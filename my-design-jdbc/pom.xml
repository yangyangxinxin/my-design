            <version>5.1.40</version>
        </dependency>
        <dependency>
            <groupId>com.mchange</groupId>
            <artifactId>c3p0</artifactId>
            <version>0.9.5-pre10</version>
        </dependency>
        <dependency>
            <groupId>com.zyl.myDesign</groupId>
            <artifactId>my-design-base-model</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
    </dependencies>

